    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "out.lst",
                "out.csv"
            ],
            "justMyCode": true
        },
        {
            "name": "PyTest",
            "type": "debugpy",
            "request": "launch",
            "stopOnEntry": false,
            "purpose": [
                "debug-test"
            ],
            "module": "pytest",
            "cwd": "${workspaceRoot}",
        }
    ]
}